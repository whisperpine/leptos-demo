{
  "rust-analyzer.rustfmt.overrideCommand": [
    "leptosfmt",
    "--stdin",
    "--rustfmt"
  ],
  "tailwindCSS.includeLanguages": {
    "rust": "html"
  },
  "files.associations": {
    "*.css": "tailwindcss",
    "LICENSE-MIT": "plaintext",
    "LICENSE-APACHE": "plaintext"
  },
  "files.readonlyInclude": {
    "LICENSE-MIT": true,
    "LICENSE-APACHE": true,
    "target/**": true,
    "dist/**": true
  },
  "cSpell.words": [
    "binaryen",
    "leptosfmt"
  ]
}
